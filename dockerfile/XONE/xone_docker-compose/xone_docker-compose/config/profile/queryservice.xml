<?xml version="1.0" encoding="utf-8"?>
<argbox>
    <service name="queryservice" library="queryservice" servicetype="queryservice">
        <argument name="log_stdout">1</argument>
        <argument name="log_level">trace</argument>
        <argument name="bind">tcp://*:8851</argument>
        <argument name="pub_bind">tcp://*:8853</argument>
        <argument name="order_sub">tcp://127.0.0.1:8852</argument>
        <argument name="broker_sub">tcp://127.0.0.1:8032</argument>
        <argument name="mysql_con">tcp://127.0.0.1:8073</argument>
        <argument name="redis_ip">127.0.0.1</argument>
        <argument name="redis_port">6380</argument>
        <argument name="redis_password">intel.com</argument>
    </service>
</argbox>
